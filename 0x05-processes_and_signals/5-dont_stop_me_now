#!/usr/bin/env bash
# This script stops 4-to_infinity_and_beyond process using kill

# Find the PID of 4-to_infinity_and_beyond process
pid=$(ps -C 4-to_infinity_and_beyond -o pid=)

# Check if the PID is not empty
if [ -n "$pid" ]; then
    # Send the SIGTERM signal to the process
    kill $pid
    # Print a message
    echo "4-to_infinity_and_beyond process stopped"
else
    # Print an error message
    echo "4-to_infinity_and_beyond process not found"
fi
